{
  "catalogs": [
    {
      "user": ".*",
      "catalog": ".*",
      "allow": "all"
    }
  ],
  "schemas": [
    {
      "user": ".*",
      "schema": ".*",
      "owner": true
    }
  ],
  "queries": [
    {
      "user": ".*",
      "allow": [
        "execute",
        "kill",
        "view"
      ]
    }
  ]
}