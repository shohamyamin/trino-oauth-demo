{
  "catalogs": [
    {
      "user": ".*",
      "catalog": ".*",
      "allow": "all"
    }
  ],
  "schemas": [
    {
      "user": ".*",
      "schema": ".*",
      "owner": true
    }
  ],
  "tables": [
    {
      "user": ".*",
      "catalog": ".*",
      "schema": ".*",
      "table": ".*",
      "privileges": ["SELECT", "INSERT", "DELETE", "UPDATE", "OWNERSHIP"]
    }
  ],
  "queries": [
    {
      "user": ".*",
      "allow": [
        "execute",
        "kill",
        "view"
      ]
    }
  ]
}